<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="com.ider.launcherpackage.launcher.MainActivity"
    android:id="@+id/content_main"
    android:layout_width="@dimen/screen_width"
    android:layout_height="match_parent"
    android:background="@drawable/back1">

    <ImageView
        android:id="@+id/vendor_logo"
        android:layout_width="200dp"
        android:layout_height="120dp"
        android:src="@drawable/logo2"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="50dp" />

    <include layout="@layout/state_layout"/>

    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_apps"
        style="@style/MainItemStyle"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="15dp"
        android:layout_width="370dp"
        android:layout_height="523dp"

        android:background="@drawable/item_bg"
        android:tag="10"
        android:nextFocusRight="@+id/folder11"/>
    <com.ider.launcherpackage.views.ShortcutFolder
        style="@style/MainItemStyle"
        android:id="@+id/folder10"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="15dp"
        android:layout_width="370dp"
        android:layout_height="523dp"
        android:visibility="gone"
        android:background="@drawable/item_bg"
        android:nextFocusRight="@+id/folder11"
        android:tag="10"
        />

    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_kodi"
        style="@style/MainItemStyle"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="307dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:visibility="gone"
        app:package_name="org.xbmc.kodi"
        android:background="@drawable/item_bg"
        android:tag="11"/>
    <com.ider.launcherpackage.views.ShortcutFolder
        style="@style/MainItemStyle"
        android:id="@+id/folder11"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="307dp"
        android:layout_width="300dp"
        android:layout_height="300dp"

        android:background="@drawable/item_bg"
        android:tag="11"
        />

    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_youtube"
        style="@style/MainItemStyle"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="529dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:tag="12"
        android:visibility="gone"
        app:package_name="com.google.android.youtube"
        android:background="@drawable/item_bg"/>

    <com.ider.launcherpackage.views.ShortcutFolder
        style="@style/MainItemStyle"
        android:id="@+id/folder12"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="529dp"
        android:layout_width="300dp"
        android:layout_height="300dp"

        android:background="@drawable/item_bg"
        android:tag="12"
        />
    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_file"
        style="@style/MainItemStyle"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="752dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:tag="13"
        android:visibility="gone"
        app:package_name="com.droidlogic.FileBrower"
        android:background="@drawable/item_bg"/>
    <com.ider.launcherpackage.views.ShortcutFolder
        style="@style/MainItemStyle"
        android:id="@+id/folder13"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="752dp"
        android:layout_width="300dp"
        android:layout_height="300dp"

        android:background="@drawable/item_bg"
        android:tag="13"
        />
    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_app_store"
        style="@style/MainItemStyle"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="974dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        android:tag="14"
        app:package_name="com.droidlogic.FileBrower"
        android:background="@drawable/item_bg"/>

    <com.ider.launcherpackage.views.ShortcutFolder
        style="@style/MainItemStyle"
        android:id="@+id/folder14"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="974dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
android:visibility="gone"
        android:background="@drawable/item_bg"
        android:tag="14"
        android:nextFocusRight="@+id/setting_display"/>
    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_google"
        style="@style/MainItemStyle"
        android:layout_marginTop="362dp"
        android:layout_marginLeft="307dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:package_name="com.android.vending"
        android:background="@drawable/item_bg"
        android:tag="15"/>

    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_media"
        style="@style/MainItemStyle"
        android:layout_marginTop="362dp"
        android:layout_marginLeft="752dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:package_name="com.android.browser"
        android:background="@drawable/item_bg"
        android:tag="17"/>
    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_setting"
        style="@style/MainItemStyle"
        android:layout_marginTop="362dp"
        android:layout_marginLeft="974dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:package_name="com.droidlogic.FileBrower"
        android:background="@drawable/item_bg"
        android:tag="18"/>

    <com.ider.launcherpackage.views.BaseImageView
        android:id="@+id/main_store"
        style="@style/MainItemStyle"
        android:layout_marginTop="362dp"
        android:layout_marginLeft="529dp"
        android:layout_width="300dp"
        android:layout_height="300dp"
        app:package_name="com.google.android.youtube"
        android:background="@drawable/item_bg"
        android:tag="16"/>
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="400dp"
        android:layout_height="80dp"
        android:layout_marginTop="640dp"
        android:layout_marginLeft="820dp"
        android:gravity="center">
        <com.ider.launcherpackage.views.BottomItem
            android:id="@+id/jia_su"
            android:layout_width="55dp"
            android:layout_height="100dp"
            android:focusable="true"
            android:tag="1"/>


        <com.ider.launcherpackage.views.BottomItem
            android:id="@+id/wifi"
            android:layout_width="55dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:focusable="true"
            android:tag="2"/>


        <com.ider.launcherpackage.views.BottomItem
            android:id="@+id/setting"
            android:layout_width="55dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:focusable="true"
            android:tag="3"/>

        <com.ider.launcherpackage.views.BottomItem
            android:id="@+id/help"
            android:layout_width="55dp"
            android:layout_height="100dp"
            android:layout_marginLeft="40dp"
            android:focusable="true"
            android:tag="4"/>

    </LinearLayout>



</FrameLayout>
