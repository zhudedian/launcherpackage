<?xml version="1.0" encoding="utf-8"?>
<com.ider.launcherpackage.views.SwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/function_main"
    android:layout_width="100dp"
    android:layout_height="match_parent"
    android:layout_gravity="right"
    android:orientation="vertical"
    android:paddingTop="100dp"
    android:paddingBottom="100dp">

    <ImageView
        android:id="@+id/setting_network"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/network_drawable"
        android:focusable="false"
        android:nextFocusLeft="@+id/folder14"
        android:layout_gravity="center_horizontal"/>
    <ImageView
        android:id="@+id/setting_display"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/display_drawable"
        android:focusable="false"
        android:nextFocusLeft="@+id/folder14"
        android:layout_gravity="center_horizontal"/>
    <ImageView
        android:id="@+id/setting_sound"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/audio_drawable"
        android:focusable="false"
        android:nextFocusLeft="@+id/folder14"
        android:layout_gravity="center_horizontal"/>
    <ImageView
        android:id="@+id/setting_apps"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/apps_drawable"
        android:focusable="false"
        android:nextFocusLeft="@+id/folder14"
        android:layout_gravity="center_horizontal"/>

    <ImageView
        android:id="@+id/setting_cleanup"
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:src="@drawable/cleanup_drawable"
        android:focusable="false"
        android:nextFocusLeft="@+id/folder14"
        android:layout_gravity="center_horizontal"/>

</com.ider.launcherpackage.views.SwipeLayout>